{
  "manifest_version": 2,
  "name": "LinkShot",
  "description": "Transforms team city build problems and stack trace links into hyperlinks",
  "version": "1.4.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "background":{
    "scripts":["background.js"],
    "persistent": false
  },

  "permissions": [
    "http://qatc.dev.wix/*"
  ]
}
