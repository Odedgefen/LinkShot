{
  "manifest_version": 2,
  "name": "LinkShot",
  "description": "Transforms screenshot links inside team city build problems into hyperlinks",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": {
    "48": "icon48.png"
  },

  "background":{
    "scripts":["background.js"],
    "persistent": false
  },

  "permissions": [
    "http://qatc.dev.wix/*"
  ]
}
