{
  "manifest_version": 2,
  "name": "LinkShot",
  "description": "Transforms team city build problems and stack trace links into hyperlinks",
  "version": "2.0",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://qatc.dev.wix/viewLog.html*"],
      "js": ["content_script.js"]
    }
  ]
}
